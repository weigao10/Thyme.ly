{
  "name": "productivity-manager",
  "version": "1.0.0",
  "description": "Productivity manager",
  "main": "main/index.js",
  "scripts": {
    "start": "electron .",
    "react-dev": "webpack -d --watch",
    "react-build": "webpack --mode production",
    "server-dev": "nodemon server/index.js",
    "rebuild": "electron-rebuild -f -w -sqlite3",
    "postinstall": "install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Romulan-Empire/productivity-manager.git"
  },
  "keywords": [
    "productivity"
  ],
  "author": "Andrew Lee, Wei Gao, Brian Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Romulan-Empire/productivity-manager/issues"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "test"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "test"
      }
    }
  },
  "homepage": "https://github.com/Romulan-Empire/productivity-manager#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "devtron": "^1.4.0",
    "electron": "^2.0.0",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.0",
    "electron-rebuild": "^1.7.3"
  },
  "dependencies": {
    "active-win": "^3.0.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "c3": "^0.5.4",
    "classificator": "^0.2.3",
    "clone": "^2.1.1",
    "d3": "^5.1.0",
    "electron-compile": "^6.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-state": "^4.1.1",
    "express": "^4.15.0",
    "fs": "0.0.1-security",
    "graceful-fs": "^4.1.11",
    "jquery": "^3.3.1",
    "knex": "^0.14.6",
    "material-ui": "^0.20.0",
    "moment": "^2.22.1",
    "moment-duration-format": "^2.2.2",
    "nodemon": "^1.17.3",
    "pg": "^7.4.2",
    "react": "^16.3.2",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-swipeable-views": "^0.12.13",
    "redux": "^4.0.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "sqlite3": "^4.0.0"
  }
